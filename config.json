{
  "calendars": [
    {
      "id": "primary",
      "name": "Primary"
    }
  ],
  "paths": {
    "clientSecret": "auth-client-secret.json",
    "token": "auth-token.json"
  },
  "server": {
    "auth": {
      "access_type": "offline",
      "scope": [
        "https://www.googleapis.com/auth/calendar.readonly"
      ]
    },
    "googleCalendarAPI": {
      "maxResults": 20,
      "orderBy": "startTime",
      "singleEvents": true,
      "timeZone": "Etc/UTC"
    },
    "port": 3000,
    "requestInterval": 30
  }
}